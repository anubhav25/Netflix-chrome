{
  "name": "Skip Intro and next epi",
  "version": "1.0",
  "description": "Nothiing fancy just skips the into and jumps to next episode!",
  "manifest_version": 2,
  "permissions": [
    "*://*.netflix.com/watch/*",
    "*://*.primevideo.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.netflix.com/watch/*",
        "*://*.primevideo.com/*"
      ],
      "js": [
        "work.js"
      ]
    }
  ]
}